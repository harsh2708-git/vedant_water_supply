<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:width="20dp"
        android:height="20dp">
        <shape android:shape="oval">
            <stroke
                android:width="2dp"
                android:color="@color/color_4A494D" />
            <solid android:color="@color/white" />
        </shape>

    </item>

    <item
        android:width="5dp"
        android:height="5dp"
        android:gravity="center">
        <shape android:shape="oval">
            <solid android:color="@color/color_4A494D" />
        </shape>

    </item>

</layer-list>